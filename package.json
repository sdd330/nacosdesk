{
  "name": "nacosdesk",
  "version": "1.0.0",
  "description": "Nacos Desktop - An easy-to-use dynamic service discovery, configuration and service management platform for building cloud native applications",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:rolldown": "rolldown -c rolldown.config.ts",
    "build:rolldown:watch": "rolldown -c rolldown.config.ts --watch",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky",
    "commit": "cz",
    "release": "standard-version",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iarna/toml": "^2.2.5",
    "@monaco-editor/loader": "^1.7.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-store": "^2.4.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "element-plus": "^2.12.0",
    "js-yaml": "^4.1.1",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.55.1",
    "pinia": "^3.0.4",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tauri-apps/cli": "^2.9.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.2",
    "@unocss/vite": "^66.5.12",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vitest/ui": "^2.1.8",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "rolldown": "1.0.0-beta.57",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.12",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^3.1.8"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
